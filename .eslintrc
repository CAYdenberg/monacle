{
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "jQuery": false,
    "$": true,
    "describe": true,
    "before": true,
    "it": true,
    "beforeAll": true,
    "globals": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": 0
  }
}
